{
    "name": "dgilan/json_doc_validator",
    "type": "library",
    "description": "Library for validation json-documents according some specific rules",
    "keywords":["json","validation","validator","rules"],
    "authors": [
        {
            "name": "Mikhail Lantukh",
            "email": "lantukhmikhail@gmail.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "Dgilan\\JsonDocValidator\\": ""
        }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/yaml": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*@dev",
        "instaclick/php-code-sniffer": "dev-master",
        "klaussilveira/phpcs-psr": "dev-master",
        "Dgilan/phpsnifferhooks": "dev-master"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "version": "dev-master",
                "name": "klaussilveira/phpcs-psr",
                "source": {
                    "url": "https://github.com/klaussilveira/phpcs-psr",
                    "type": "git",
                    "reference": "master"
                },
                "dist": {
                    "url": "https://github.com/klaussilveira/phpcs-psr/archive/master.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "version": "dev-master",
                "name": "Dgilan/phpsnifferhooks",
                "source": {
                    "url": "https://bitbucket.org/Dgilan/phpsnifferhooks",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ]

}